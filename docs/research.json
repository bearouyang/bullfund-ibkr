            "post": {
                "summary": "Get Contract Details",
                "description": "Get detailed contract information using the thread-safe sync wrapper.",
                "operationId": "get_contract_details_api_v1_research_contract_details_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ContractRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
